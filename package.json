{
  "name": "cookie-manager",
  "version": "1.0.0",
  "description": "A brower extension for managing cookies",
  "main": "index.js",
  "keywords": [
    "cookie-manager",
    "cookie",
    "chrome",
    "browser",
    "browser-extension",
    "chrome-extension"
  ],
  "author": "Fog3211",
  "license": "MIT",
    "scripts": {
    "build": "pnpm build:chrome",
    "build:chrome": "cross-env BROWSER=chrome vite build",
    "build:firefox": "cross-env BROWSER=firefox vite build",
    "package": "node utils/package.js",
    "dev:chrome": "cross-env MINIFY=false BROWSER=chrome vite build --watch",
    "dev:firefox": "cross-env MINIFY=false BROWSER=firefox vite build --watch",
    "start": "yarn dev:chrome",
    "start:none": "yarn dev:chrome",
    "start:chrome": "cross-env HC_AUTOLAUNCH=menace yarn dev:chrome",
    "start:firefox": "cross-env HC_AUTOLAUNCH=menace yarn dev:firefox",
    "format:check": "eslint ."
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@mdi/js": "^7.2.96",
    "@sveltejs/vite-plugin-svelte": "^2.2.0",
    "@tsconfig/svelte": "^4.0.1",
    "@types/chrome": "^0.0.235",
    "@types/download": "^8.0.2",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5.59.5",
    "autoprefixer": "^10.4.14",
    "body-parser": "1.20.2",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "download": "^8.0.0",
    "eslint": "^8",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2",
    "eslint-plugin-node": "^11",
    "eslint-plugin-promise": "6",
    "eslint-plugin-svelte3": "^4.0.0",
    "express": "^4.18.2",
    "iframe-translator": "^0.3.6",
    "is-dark-color": "^1.2.0",
    "postcss": "^8.4.23",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^15.1.0",
    "postcss-input-range": "^5.0.0",
    "postcss-preset-env": "^8.3.2",
    "postcss-url": "^10.1.3",
    "rollup-plugin-copy": "^3.4.0",
    "safe-compare": "^1.1.4",
    "sha-1": "^1.0.0",
    "smelte": "^1.1.8",
    "svelte": "^3.59.1",
    "svelte-check": "^3.3.2",
    "svelte-preprocess": "^5.0.3",
    "svelte-waypoint": "^0.1.4",
    "svelte-webext-stores": "^0.0.13",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-web-extension": "^3.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Fog3211/cookie-manager"
  },
  "engines": {

  },
  "packageManager": "pnpm@8.3.1"
}
