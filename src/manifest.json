{
  "manifest_version": 3,
  "name": "Cookie Manager by Fog3211",
  "homepage_url": "https://github.com/Fog3211/cookie-manager",
  "description": "A brower extension for managing cookies",
  "version": "1.0.0",
  "permissions": [
    "storage",
    "cookies"
  ],
  "icons": {
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "action": {
    "default_icon": {
      "48": "48x48.png",
      "128": "128x128.png"
    },
    "default_title": "cookie-manager",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": [ "*" ],
      "matches": [ "<all_urls>" ]
    }
  ],
  "{{chrome}}.incognito": "split"
}
